---
const name = 'Astro'
const style = 'purple'
const response = await fetch(
  'https:/jsonplaceholder.typicode.com/users?_limit=10'
)
const users = ['Astro', 'Remy', 'John', 'Mary']
const more_users = await response.json()
const visible = false
---

<>
  <h1 class={style}>Hello {name}</h1>

  <ul>
    {users.map((user) => <li>{user}</li>)}
  </ul>
  <ul>
    {more_users.map((user: any) => <li>{user.name}</li>)}
  </ul>
  {visible && <p>I am visible</p>}
  {visible ? <p>I am not visible</p> : <p>I am not visible</p>}
</>

<style>
  h1 {
    color: red;
  }
  .purple {
    color: purple;
  }
</style>
